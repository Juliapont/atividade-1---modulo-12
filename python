# 1. Criação do dicionário vazio
pessoas = {}

# 2. Adicionando pessoas e suas idades ao dicionário
pessoas["Alice"] = 30
pessoas["Bob"] = 25
pessoas["Charlie"] = 35

# 3. Mostrando as informações de todas as pessoas
for nome, idade in pessoas.items():
    print(f"Nome: {nome}, Idade: {idade}")
